#include <stdio.h>

int main() {
    int grades = 7;
    
    printf("Enter a number (1-8) representing a number of the grades: ");
    scanf("%d", &grades);
    
    switch (grades) {
    case 1:
    printf("100");
    break;
    
    case 2:
    printf("90");
    break;
    
    case 3:
    printf("80");
    break;
    
    case 4:
    printf("70");
    break;
    
    case 5:
    printf("60");
    break;
    
    case 6:
    printf("50");
    break;
    
    case 7:
    printf("40");
    break;
    
    case 8:
    printf("0");
    break;
    
    default:
    printf("Invalid input. Please enter a number between 1 and 8.\n");
    break;
    
    }
    

    return 0;
}
